{
  "name": "ec.admin",
  "version": "0.1.4",
  "private": false,
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/entrecode/ec.admin.git"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "prebuild": "yarn clean",
    "build": "tsc --build \"./tsconfig.json\"",
    "deck": "mdx-deck demo/deck.mdx",
    "demo": "parcel demo/index.html --https --open",
    "doc": "cd doc && yarn start",
    "deploy-doc": "cd doc && GIT_USER=felixroos yarn deploy",
    "deps": "cd src && depcruise --exclude \"node_modules\" --output-type dot . | dot -T svg > ../doc/static/img/dependencygraph.svg"
  },
  "peerDependencies": {
    "ec.sdk": "^0.22.2",
    "ra-customizable-datagrid": "^1.1.0",
    "react-admin": "^3.9.5"
  },
  "devDependencies": {
    "@code-surfer/themes": "^3.1.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "code-surfer": "^3.1.1",
    "dependency-cruiser": "^9.17.1",
    "ec.sdk": "^0.22.2",
    "jest": "^26.6.3",
    "mdx-deck": "4.0.0",
    "parcel": "^1.12.4",
    "ra-customizable-datagrid": "^1.1.0",
    "react": "^16.9.53",
    "react-admin": "^3.9.5",
    "react-dom": "^16.9.53",
    "svg-inline-react": "^3.2.0",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "ra-language-german": "^2.1.2"
  }
}
